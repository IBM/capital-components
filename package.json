{
  "name": "@fss/components",
  "version": "0.2.0",
  "description": "",
  "keywords": [],
  "main": "dist/wfss-components.umd.js",
  "module": "dist/wfss-components.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Paul Sachs <paul.sachs@ibm.com>",
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/wfss-finance/wfss-components.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && cp -rf css-gridish/css dist/css",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "gen:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "prepublish": "yarn build"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "carbon-components": "^8.10.0",
    "carbon-components-react": "^5.43.1",
    "carbon-icons": "^6.0.0",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "css-gridish": "^2.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "emotion": "^9.1.3",
    "husky": "^0.14.0",
    "jest": "^22.0.2",
    "lint-staged": "^7.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.4.4",
    "prompt": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.3",
    "react-intl": "^2.4.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.57.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "ts-jest": "^22.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.0",
    "typescript": "^2.8.3",
    "validate-commit-msg": "^2.12.2"
  },
  "peerDependencies": {
    "carbon-components-react": "5.x",
    "emotion": "9.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-intl": "2.x"
  },
  "publishConfig": {
    "registry": "https://na.artifactory.swg-devops.com:443/artifactory/api/npm/ip-wfss-npm-local"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
