{
  "name": "capital-components",
  "version": "1.20.0",
  "description": "",
  "keywords": [],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "styles",
    "types"
  ],
  "author": "Paul Sachs <paul.sachs@ibm.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/capital-components.git"
  },
  "license": "Apache-2",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "build": "gulp build",
    "start": "start-storybook -p 6007",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release",
    "deploy-docs": "build-storybook && gh-pages -d storybook-static"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-components"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"yarn run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)|test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "__snapshots__"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.tsx",
      "src/components/**/*.ts",
      "src/utils/**/*.tsx",
      "src/utils/**/*.ts",
      "!src/components/**/stories.tsx",
      "!src/components/Popover/**",
      "!src/components/BleedingRow/**",
      "!src/components/DropMenu/**",
      "!src/components/DropMenuItem/**",
      "!src/components/FlyOver/**",
      "!src/components/FlyOverProvider/**",
      "!src/components/Grid/**",
      "!src/components/Col/**",
      "!src/components/Table/**",
      "!src/components/Tab/**",
      "!src/components/Tabs/**",
      "!src/components/TabsV2/**",
      "!src/components/ScrollRow/**",
      "!src/components/**/*_hooks.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 85,
        "lines": 85,
        "statements": 90
      }
    },
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/test-helpers/setup.ts",
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "moduleNameMapper": {
      "capital-components": "<rootDir>/src",
      "\\.(css|jpg|png)$": "<rootDir>/test-setup/empty-module.js"
    }
  },
  "devDependencies": {
